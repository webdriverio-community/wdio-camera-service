{
  "name": "wdio-camera-service",
  "version": "1.3.2",
  "author": "Vince Graics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio-community/wdio-camera-service.git"
  },
  "keywords": [
    "wdio-plugin",
    "wdio-service",
    "webdriverio",
    "camera",
    "mock"
  ],
  "type": "module",
  "main": "./build/types/index.js",
  "module": "./build/types/index.js",
  "types": "./build/types/index.d.ts",
  "files": [
    "build",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --fix",
    "prebuild": "rimraf build --glob ./*.tgz",
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:local": "pnpm run build && cd sandbox && pnpm i && pnpm test",
    "prepare": "husky"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.16",
    "@wdio/eslint": "^0.1.3",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "rimraf": "^6.1.2",
    "typescript": "5.9",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@wdio/globals": ">=9.0.0",
    "@wdio/types": ">=9.0.0",
    "webdriverio": ">=9.0.0"
  },
  "dependencies": {
    "@types/node": "^24.0.15"
  }
}
