{
  "name": "wdio-camera-service",
  "version": "1.0.1",
  "author": "Vince Graics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio-community/wdio-camera-service.git"
  },
  "keywords": [
    "wdio-plugin",
    "wdio-service",
    "webdriverio",
    "camera",
    "mock"
  ],
  "type": "module",
  "main": "./build/types/index.js",
  "module": "./build/types/index.js",
  "types": "./build/types/index.d.ts",
  "files": [
    "build",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --fix",
    "prebuild": "rimraf build --glob ./*.tgz",
    "build": "tsc",
    "postbuild": "npm pack",
    "test:local:win": "npm run build && cd sandbox && npm rm %npm_package_name% && npm install ../wdio-camera-service-%npm_package_version%.tgz",
    "test:local:ux": "npm run build && cd sandbox && npm rm $npm_package_name && npm install ../wdio-camera-service-$npm_package_version.tgz",
    "prepare": "husky"
  },
  "devDependencies": {
    "@wdio/eslint": "^0.1.2",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "typescript": "5.8"
  },
  "peerDependencies": {
    "@wdio/globals": ">=9.0.0",
    "@wdio/types": ">=9.0.0",
    "webdriverio": ">=9.0.0"
  }
}
